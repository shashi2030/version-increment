{
  "name": "custom-app",
  "version": "1.0.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --hot",
    "build": "npm run patch && webpack --mode production ",
    "build-local": "serve --single ./build",
    "major": "verup 1.0.0",
    "minor": "verup 1.0",
    "patch": "verup 1",
    "serve": "serve --single ./build"
  },
  "keywords": [
    "benchmark"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "serve": "^11.0.0",
    "style-loader": "^3.3.1",
    "typed.js": "^2.0.10",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "verup": "^1.7.0"
  },
  "extra": {
    "verup": {
      "files": [
        "package-lock.json"
      ],
      "regs": [
        "((?:\\$|(?:\\s*\\*?\\s*@)|(?:^\\s*(?:var|,)?\\s+))ver(?:sion)?[\\s\\:='\"]+)([0-9]+(?:\\.[0-9]+){2,2})",
        "^(\\s*\\$(?:_)?version[\\s='\"]+)([0-9]+(?:\\.[0-9]+){2,2})",
        "^(\\s?\\*.*v)([0-9]+(?:\\.[0-9]+){2,2})"
      ]
    }
  }
}
